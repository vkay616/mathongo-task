    <script>
      generateContent();
      
      function generateContent() {
        
        let jsonData = sessionStorage.getItem('json');
        jsonData = JSON.parse(jsonData);

        // set page title for pdf name
        document.title = `question_marks`;

        renderData(jsonData);

        // open solution page
        let URL = `./solution`;
        window.open(URL, '_blank');

        function renderData(data) {
          console.log('Render function called');
          console.log(data);

          const page = document.getElementById('page');
          const answerKey = document.getElementById('answerKey');

          for (let i = 0; i < data.length; i++) {
            if (typeof data[i].question.text !== 'string') {
              data[i].question.text = JSON.stringify(data[i].question.text);
            }
            if (typeof data[i].solution.text !== 'string') {
              data[i].solution.text = JSON.stringify(data[i].solution.text);
            }

            if (data[i].type !== 'numerical') {
              if (typeof data[i].options[0].text !== 'string') {
                data[i].options[0].text = JSON.stringify(data[i].options[0].text);
              }

              if (typeof data[i].options[1].text !== 'string') {
                data[i].options[1].text = JSON.stringify(data[i].options[1].text);
              }
              if (typeof data[i].options[2].text !== 'string') {
                data[i].options[2].text = JSON.stringify(data[i].options[2].text);
              }
              if (typeof data[i].options[3].text !== 'string') {
                data[i].options[3].text = JSON.stringify(data[i].options[3].text);
              }
            } else {
              if (typeof data[i].correctValue !== 'string') {
                data[i].correctValue = JSON.stringify(data[i].correctValue);
              }
            }

            let qType = "";

            if (data[i].type === "singleCorrect") {
              qType = "Single Correct"
            } else if (data[i].type === "numerical") {
              qType = "Numerical"
            } else {
              qType = "Multiple Correct"
            }

            // single correct
            if (data[i].type !== 'numerical') {
              if (i == 0) {
                // question
                page.innerHTML += `
                                <div class="col-12 mb-1">
                                    <span style="font-size: 14px" class="badge">Q${i + 1}</span>
                                </div>
                                <div class="col-12 mb-2">
                                    ${data[i].question.text.replace(/\n/g, '<br>')}
                                </div>
                            `;
              } else {
                // question
                page.innerHTML += `
                                <div class="col-12 mt-4 mb-1">
                                  <span style="font-size: 14px" class="badge">Q${i + 1}</span>
                                </div>
                                <div class="col-12 mb-2">
                                    ${data[i].question.text.replace(/\n/g, '<br>')}
                                </div>
                            `;
              }
              // options
              page.innerHTML += `<div class="col-12">(1) ${data[i].options[0].text.replace(/\n/g, '<br>')}</div>`;
              page.innerHTML += `<div class="col-12">(2) ${data[i].options[1].text.replace(/\n/g, '<br>')}</div>`;
              page.innerHTML += `<div class="col-12">(3) ${data[i].options[2].text.replace(/\n/g, '<br>')}</div>`;
              page.innerHTML += `<div class="col-12 mb-5">(4) ${data[i].options[3].text.replace(/\n/g, '<br>')}</div>`;
            } else {
              // numerical
              if (i == 0) {
                // question
                page.innerHTML += `
                                <div class="col-12 mb-1">
                                  <span style="font-size: 14px" class="badge">Q${i + 1}</span>
                                </div>
                                <div class="col-12 mb-5">
                                    ${data[i].question.text.replace(/\n/g, '<br>')}
                                </div>
                            `;
              } else {
                // numeric question
                page.innerHTML += `
                                <div class="col-12 mt-4 mb-1">
                                  <span style="font-size: 14px" class="badge">Q${i + 1}</span>
                                </div>
                                <div class="col-12 mb-5">
                                    ${data[i].question.text.replace(/\n/g, '<br>')}
                                </div>
                            `;
              }
            }
          } // loop

          for (let i = 0; i < data.length; i++) {
            if (data[i].type == 'numerical') {
              answerKey.innerHTML += `<div style="margin-top: 1rem; margin-right: 2rem;"><strong>Q${i + 1}</strong> (${data[i].correctValue})</div>`;
            } else {
              let correctOptions = "";

              if (data[i].options[0].isCorrect) {
                correctOptions += "(1)    " 
              }

              if (data[i].options[1].isCorrect) {
                correctOptions += "(2)    "
              }

              if (data[i].options[2].isCorrect) {
                correctOptions += "(3)    "
              }

              if (data[i].options[3].isCorrect) {
                correctOptions += "(4)    "
              }

              answerKey.innerHTML += `<div style="margin-top: 1rem; margin-right: 2rem;"><strong>Q${i + 1}</strong> ${correctOptions}</div>`;
            }
          }
        } // render function

        MathJax.Hub.Queue(['Typeset', MathJax.Hub]);
      } // generate function
